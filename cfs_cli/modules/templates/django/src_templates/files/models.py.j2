from django.db import models

from {{package_name}}_mixins.models import BaseModel


class FileKeys(BaseModel):
    key_name = models.CharField(editable=False, max_length=1200)
    key_file_id = models.CharField(max_length=1200, editable=False)
    key_is_active = models.BooleanField(default=True, editable=False)
    key_recorded_on = models.DateTimeField(auto_now_add=True)

    class Meta:
        db_table = '{{package_name}}_file_keys'
        ordering = ['-primary_key']
        verbose_name_plural = 'FILE KEYS'

    def __str__(self):
        return '{}'.format(self.key_file_id)